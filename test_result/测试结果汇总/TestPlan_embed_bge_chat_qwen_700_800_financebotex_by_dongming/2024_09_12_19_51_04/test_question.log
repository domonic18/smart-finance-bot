WITH FundHoldings AS (
    SELECT
        "基金代码",
        "对应股票代码",
        SUM("市值") AS TotalMarketValue
    FROM
        "基金可转债持仓明细"
    WHERE
        "基金简称" = '交银施罗德可转债债券C'
        AND "持仓日期" = '20200331'
    GROUP BY
        "基金代码", "对应股票代码"
),
IndustryHoldings AS (
    SELECT
        fh."基金代码",
        fh."对应股票代码",
        ih."一级行业名称",
        fh.TotalMarketValue
    FROM
        FundHoldings fh
        JOIN "A股公司行业划分表" ih ON fh."对应股票代码" = ih."股票代码"
        AND ih."交易日期" = (SELECT MAX("交易日期") FROM "A股公司行业划分表" WHERE ih."股票代码" = fh."对应股票代码")
),
TotalMarketValue AS (
    SELECT
        "基金代码",
        SUM(TotalMarketValue) AS TotalFundMarketValue
    FROM
        FundHoldings
    GROUP BY
        "基金代码"
)
SELECT
    ih."一级行业名称",
    SUM(ih.TotalMarketValue) / tmv.TotalFundMarketValue * 100 AS Percentage
FROM
    IndustryHoldings ih
    JOIN TotalMarketValue tmv ON ih."基金代码" = tmv."基金代码"
GROUP BY
    ih."一级行业名称"
ORDER BY
    Percentage DESC
LIMIT 1;
```
